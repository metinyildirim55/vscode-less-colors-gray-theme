        "settings.modifiedItemIndicator": "#545454",
        "welcomePage.buttonBackground": "#343434",
        "welcomePage.buttonHoverBackground": "#4b4b4b"
    },
    "semanticHighlighting": true,
    "tokenColors": [
        {
            "scope": ["comment", "punctuation.definition.comment", "string.comment"],
            "settings": {
                "foreground": "#717171"
            }
        },
        {
            "scope": ["constant", "entity.name.constant", "variable.other.constant", "variable.language"],
            "settings": {
                "foreground": "#acacac"
            }
        },
        {
            "scope": ["entity", "entity.name"],
            "settings": {
                "foreground": "#a6a6a6"
            }
        },
        {
            "scope": "variable.parameter.function",
            "settings": {
                "foreground": "#505050"
            }
        },
        {
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#c2c2c2"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#9e9e9e"
            }
        },
        {
            "scope": ["storage", "storage.type"],
            "settings": {
                "foreground": "#9e9e9e"
            }
        },
        {
            "scope": ["storage.modifier.package", "storage.modifier.import", "storage.type.java"],
            "settings": {
                "foreground": "#505050"
            }
        },
        {
            "scope": ["string", "punctuation.definition.string", "string punctuation.section.embedded source"],
            "settings": {
                "foreground": "#c3c3c3"
            }
        },
        {
            "scope": "support",
            "settings": {
                "foreground": "#acacac"
            }
        },
        {
            "scope": "meta.property-name",
            "settings": {
                "foreground": "#acacac"
            }
        },
        {
            "scope": "variable",
            "settings": {
                "foreground": "#bdbdbd"
            }
        },
        {
            "scope": "variable.other",
            "settings": {
                "foreground": "#505050"
            }
        },
        {
            "scope": "invalid.broken",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#c6c6c6"
            }
        },
        {
            "scope": "invalid.deprecated",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#c6c6c6"
            }
        },
        {
            "scope": "invalid.illegal",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#c6c6c6"
            }
        },
        {
            "scope": "invalid.unimplemented",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#c6c6c6"
            }
        },
        {
            "scope": "carriage-return",
            "settings": {
                "fontStyle": "italic underline",
                "background": "#9e9e9e",
                "foreground": "#282828",
                "content": "^M"
            }
        },
        {
            "scope": "message.error",
            "settings": {
                "foreground": "#c6c6c6"
            }
        },
        {
            "scope": "string source",
            "settings": {
                "foreground": "#505050"
            }
        },
        {
            "scope": "string variable",
            "settings": {
                "foreground": "#acacac"
            }
        },
        {
            "scope": ["source.regexp", "string.regexp"],
            "settings": {
                "foreground": "#363636"
            }
        },
        {
            "scope": ["string.regexp.character-class", "string.regexp constant.character.escape", "string.regexp source.ruby.embedded", "string.regexp string.regexp.arbitrary-repitition"],
            "settings": {
                "foreground": "#363636"
            }
        },
        {
            "scope": "string.regexp constant.character.escape",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#c2c2c2"
            }
        },
        {
            "scope": "support.constant",
            "settings": {
                "foreground": "#acacac"
            }
        },
        {
            "scope": "support.variable",
            "settings": {
                "foreground": "#acacac"
            }
        },
        {
            "scope": "meta.module-reference",
            "settings": {
                "foreground": "#acacac"
            }
        },
        {
            "scope": "punctuation.definition.list.begin.markdown",
            "settings": {
                "foreground": "#bdbdbd"
            }
        },
        {
            "scope": ["markup.heading", "markup.heading entity.name"],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#acacac"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "foreground": "#c2c2c2"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic",
                "foreground": "#505050"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#505050"
            }
        },
        {
            "scope": "markup.raw",
            "settings": {
                "foreground": "#acacac"
            }
        },
        {
            "scope": ["markup.deleted", "meta.diff.header.from-file", "punctuation.definition.deleted"],
            "settings": {
                "background": "#393939",
                "foreground": "#c6c6c6"
            }
        },
        {
            "scope": ["markup.inserted", "meta.diff.header.to-file", "punctuation.definition.inserted"],
            "settings": {
                "background": "#323232",
                "foreground": "#c2c2c2"
            }
        },
        {
            "scope": ["markup.changed", "punctuation.definition.changed"],
            "settings": {
                "background": "#686868",
                "foreground": "#bdbdbd"
            }
        },
        {
            "scope": ["markup.ignored", "markup.untracked"],
            "settings": {
                "foreground": "#343434",
                "background": "#acacac"
            }
        },
        {
            "scope": "meta.diff.range",
            "settings": {
                "foreground": "#a6a6a6",
                "fontStyle": "bold"
            }
        },
        {
            "scope": "meta.diff.header",
            "settings": {
                "foreground": "#acacac"
            }
        },
        {
            "scope": "meta.separator",
            "settings": {
                "fontStyle": "bold",
                "foreground": "#acacac"
            }
        },
        {
            "scope": "meta.output",
            "settings": {
                "foreground": "#acacac"
            }
        },
        {
            "scope": ["brackethighlighter.tag", "brackethighlighter.curly", "brackethighlighter.round", "brackethighlighter.square", "brackethighlighter.angle", "brackethighlighter.quote"],
            "settings": {
                "foreground": "#7d7d7d"
            }
        },
        {
            "scope": "brackethighlighter.unmatched",
            "settings": {
                "foreground": "#c6c6c6"
            }
        },
        {
            "scope": ["constant.other.reference.link", "string.other.link"],
            "settings": {
                "foreground": "#363636",
                "fontStyle": "underline"
            }
        }
    ]
}
