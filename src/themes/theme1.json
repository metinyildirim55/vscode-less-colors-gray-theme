        "editor.focusedStackFrameHighlightBackground": "#8888888",
        "peekViewEditor.matchHighlightBackground": "#cfcfcf33",
        "peekViewResult.matchHighlightBackground": "#cfcfcf33",
        "peekViewEditor.background": "#22222288",
